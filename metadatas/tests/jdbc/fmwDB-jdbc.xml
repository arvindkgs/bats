<?xml version="1.0" encoding="UTF-8"?>
<jdbc-data-source xsi:schemaLocation="http://xmlns.oracle.com/weblogic/jdbc-data-source http://xmlns.oracle.com/weblogic/jdbc-data-source/1.2/jdbc-data-source.xsd" xmlns="http://xmlns.oracle.com/weblogic/jdbc-data-source" xmlns:sec="http://xmlns.oracle.com/weblogic/security" xmlns:wls="http://xmlns.oracle.com/weblogic/security/wls" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <name>fmwDB</name>
  <jdbc-driver-params>
    <url>jdbc:oracle:thin:@FA_DEFAULT</url>
    <driver-name>oracle.jdbc.OracleDriver</driver-name>
    <properties>
      <property>
        <name>user</name>
        <value>fmw_runtime</value>
      </property>
      <property>
        <name>oracle.net.CONNECT_TIMEOUT</name>
        <value>120000</value>
      </property>
      <property>
        <name>ORACLE.JDBC.READTIMEOUT</name>
        <value>585000</value>
      </property>
      <property>
        <name>oracle.jdbc.implicitStatementCacheSize</name>
        <value>10</value>
      </property>
      <property>
        <name>oracle.jdbc.maxCachedBufferSize</name>
        <value>19</value>
      </property>
      <property>
        <name>v$session.process</name>
        <value>fmwDB</value>
      </property>
      <property>
        <name>v$session.program</name>
        <value>null</value>
      </property>
      <property>
        <name>oracle.jdbc.ReadTimeout</name>
        <value>595000</value>
      </property>
    </properties>
    <password-encrypted>{AES}Q/kAO45GFx1Hr+ECPCPSp2mW0KodDOe0ELWskZkRGQg=</password-encrypted>
  </jdbc-driver-params>
  <jdbc-connection-pool-params>
    <initial-capacity>0</initial-capacity>
    <max-capacity>100</max-capacity>
    <min-capacity>2</min-capacity>
    <capacity-increment>2</capacity-increment>
    <shrink-frequency-seconds>7200</shrink-frequency-seconds>
    <connection-creation-retry-frequency-seconds>60</connection-creation-retry-frequency-seconds>
    <test-frequency-seconds>300</test-frequency-seconds>
    <test-connections-on-reserve>true</test-connections-on-reserve>
    <test-table-name>SQL SELECT 1 FROM DUAL</test-table-name>
    <login-delay-seconds>0</login-delay-seconds>
    <init-sql>SQL SELECT 1 FROM DUAL</init-sql>
    <statement-cache-size>0</statement-cache-size>
    <remove-infected-connections>true</remove-infected-connections>
    <seconds-to-trust-an-idle-pool-connection>15</seconds-to-trust-an-idle-pool-connection>
  </jdbc-connection-pool-params>
  <jdbc-data-source-params>
    <jndi-name>jdbc/activities/CustomPortalDS</jndi-name>
