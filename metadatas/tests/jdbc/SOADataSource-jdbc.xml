<?xml version="1.0" encoding="UTF-8"?>
<jdbc-data-source xsi:schemaLocation="http://xmlns.oracle.com/weblogic/jdbc-data-source http://xmlns.oracle.com/weblogic/jdbc-data-source/1.2/jdbc-data-source.xsd" xmlns="http://xmlns.oracle.com/weblogic/jdbc-data-source" xmlns:sec="http://xmlns.oracle.com/weblogic/security" xmlns:wls="http://xmlns.oracle.com/weblogic/security/wls" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <name>SOADataSource</name>
  <jdbc-driver-params>
    <url>jdbc:oracle:thin:@FA_XA</url>
    <driver-name>oracle.jdbc.xa.client.OracleXADataSource</driver-name>
    <properties>
      <property>
        <name>user</name>
        <value>FA_FUSION_SOAINFRA</value>
      </property>
      <property>
        <name>oracle.net.CONNECT_TIMEOUT</name>
        <value>120000</value>
      </property>
      <property>
        <name>oracle.jdbc.implicitStatementCacheSize</name>
        <value>10</value>
      </property>
      <property>
        <name>oracle.jdbc.maxCachedBufferSize</name>
        <value>19</value>
      </property>
      <property>
        <name>v$session.process</name>
        <value>SOADataSource</value>
      </property>
      <property>
        <name>v$session.program</name>
        <value>null</value>
      </property>
      <property>
        <name>oracle.jdbc.ReadTimeout</name>
        <value>595000</value>
      </property>
