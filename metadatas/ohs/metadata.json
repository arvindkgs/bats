{
  "name": "OHS Checks",
  "description": "Post resizing validation checks for WLS (JDBS), JVM (WLS Cluster) Resizing, and OHS Tuning.",
  "logFileName": "metadatas/ohs/ohs.log",
  "options": "test",
  "tests": [
    {
      "name": "OHS Test",
      "checks": [
        {
          "name": "Single Compare",
          "type": "COMPARE",
          "source": {
            "property": "topology.ohsTunings.MaxClients",
            "file": "metadatas/ohs/source.json",
            "type": "JSON"
          },
          "target": {
            "property": "$.IfModule..mpm_worker_module..MaxClients",
            "file": "metadatas/ohs/httpd.conf",
            "type": "CONF"
          }
        }
      ]
    }
  ]
}